# REIA Environment Configuration
# Copy this file to .env and adjust values as needed

# Application Configuration
MAX_PROCESSES=2
# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL (default: INFO)
LOG_LEVEL=INFO

# REIA Database Configuration  
DB_NAME=reia_db
DB_USER=reia_user
DB_PASSWORD=reia_password

POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

POSTGRES_POOL_SIZE=5
POSTGRES_MAX_OVERFLOW=10

# OpenQuake Configuration
OQ_HOST=http://localhost:8800
OQ_USER=secret
OQ_PASSWORD=secret
OQ_ADMIN_EMAIL=user@domain.ch
OQ_PORT=8800
OQ_VERSION=16


ALLOW_ORIGINS=["http://localhost","http://localhost:5000"]
ALLOW_ORIGIN_REGEX=""

CSV_NAMES_PATH=reia/config/csv_settings.json

ROOT_PATH=/reiaws
WEB_CONCURRENCY=1

AGENCY_ID=